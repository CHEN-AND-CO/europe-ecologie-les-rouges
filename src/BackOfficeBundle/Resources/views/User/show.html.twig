{% extends "@App/template.html.twig" %}

{% block title %} Liste des utilisateurs {% endblock %}
{% block content %}
    <h1> {{user.getNom()}} {{user.getPrenom()}} </h1>
    <h2> {{user.getAdresse()}} </h2>
    <h2> {{user.getCreated()}} </h2>
    <h2> {{user.getUpdated()}} </h2>
    <h2> {{user.getService()}} </h2>
    <h2> {{user.getSociete()}} </h2>
    <h2> {{user.getVille()}} </h2>
    
    <a class="button" href="{{ path('back_office_user_delete', {'id': user.getId()})}}"> Delete </a>
    <br>
    <a class="button" href="{{ path('back_office_user_list') }}"> Retour </a>
{% endblock %}