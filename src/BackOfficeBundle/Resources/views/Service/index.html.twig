{% extends "@App/template.html.twig" %}

{% block title %} Liste des Services {% endblock %}
{% block content %}
    <h1> Liste des Services </h1>
    <ul id="params">
        {% for service in services %}
            <li>
                <a href="{{ path('back_office_service_show', {'id': service.getId()}) }}">  {{ service.getService() }} </a>
            </li>
            <br>
        {% endfor %}
    </ul>
    <a class="button" id="btn-add" href="{{ path('back_office_societe_create') }}">Ajouter</a>
{% endblock %}
